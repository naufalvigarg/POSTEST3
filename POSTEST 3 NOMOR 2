import os
import time

while True:
    try:
        os.system('cls' if os.name == 'nt' else 'clear')
        #NIM: 2009106002 + 10 = 12
        angka = int(input("Masukkan angka: "))
        y = 1
        x = 1
        while (y <= angka):
            print (x)
            x += 1
            if (x == 10):
                x -= 9
            y += 1
        break
    except ValueError:
        print("Angka yang anda input salah!")
        time.sleep(1.5)
